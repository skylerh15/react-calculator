{
    "name": "react-calculator",
    "version": "0.1.0",
    "private": true,
    "homepage": "https://skylerh15.github.io/react-calculator",
    "dependencies": {
        "@xstate/react": "^0.7.0",
        "gh-pages": "2.0.1",
        "lodash": "^4.17.21",
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "react-scripts": "^3.0.1",
        "styled-components": "^4.3.2",
        "ts-loader": "^6.0.4",
        "tslint": "^5.18.0",
        "tslint-react": "^4.0.0",
        "typescript": "^3.5.3",
        "webpack": "4.29.6",
        "xstate": "^4.6.7"
    },
    "devDependencies": {
        "@types/jest": "^24.0.16",
        "@types/lodash": "^4.14.136",
        "@types/node": "^12.6.9",
        "@types/react": "^16.8.24",
        "@types/react-dom": "^16.8.5",
        "@types/styled-components": "^4.1.18",
        "css-loader": "^3.1.0",
        "husky": "^3.0.2",
        "prettier": "^1.18.2",
        "pretty-quick": "^1.11.1",
        "style-loader": "^0.23.1",
        "tslint-config-prettier": "^1.18.0",
        "typescript-plugin-styled-components": "^1.4.3"
    },
    "scripts": {
        "build": "yarn && react-scripts build",
        "eject": "react-scripts eject",
        "prettify": "prettier --write \"src/**/*.{ts,tsx}\"",
        "start": "yarn && react-scripts start",
        "predeploy": "yarn run build",
        "deploy": "gh-pages -d build",
        "test": "react-scripts test",
        "tslint-check": "tslint-config-prettier-check ./tslint.json"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged"
        }
    }
}
